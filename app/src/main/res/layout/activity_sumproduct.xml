<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="sdu.thanaset.casephone.Sumproduct">

    <ListView
        android:id="@+id/sum2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="0dp"
        app:layout_constraintBottom_toTopOf="@+id/Highlightbtn"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/toolbar5" />

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar5"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@color/colorAccent"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:text="sum"
        android:textColor="@android:color/black"
        android:textSize="35sp"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar5"
        app:layout_constraintLeft_toRightOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/toolbar5"
        android:layout_marginStart="8dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/iconppp"
        app:layout_constraintTop_toTopOf="@+id/toolbar5"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar5"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="8dp" />

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@color/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <ImageView
        android:id="@+id/Homebnt"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginLeft="24dp"
        android:layout_marginStart="24dp"
        android:onClick="homebnt"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar3"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/toolbar3"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/a" />

    <ImageView
        android:id="@+id/Searchbnt"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginLeft="24dp"
        android:layout_marginStart="24dp"
        android:onClick="searchbnt"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/Homebnt"
        app:layout_constraintTop_toTopOf="@+id/toolbar3"
        app:srcCompat="@drawable/b" />

    <ImageView
        android:id="@+id/Highlightbtn"
        android:onClick="highlightbtn"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/iconppp"
        app:layout_constraintLeft_toRightOf="@+id/Searchbnt"
        android:layout_marginLeft="24dp"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar3"
        app:layout_constraintTop_toTopOf="@+id/toolbar3"
        android:layout_marginStart="24dp" />

    <ImageView
        android:id="@+id/Basketbtn"
        android:onClick="basketbtn"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/d"
        app:layout_constraintLeft_toRightOf="@+id/Highlightbtn"
        android:layout_marginLeft="24dp"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar3"
        app:layout_constraintTop_toTopOf="@+id/toolbar3"
        android:layout_marginStart="24dp" />

    <ImageView
        android:id="@+id/imageView7"
        android:onClick="paybtn"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/f"
        app:layout_constraintLeft_toRightOf="@+id/Basketbtn"
        android:layout_marginLeft="24dp"
        app:layout_constraintTop_toTopOf="@+id/toolbar3"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginStart="24dp" />

</android.support.constraint.ConstraintLayout>
