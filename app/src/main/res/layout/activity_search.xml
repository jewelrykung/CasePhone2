<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="sdu.thanaset.casephone.Search">

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@color/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@color/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <ImageView
        android:id="@+id/Homebnt"
        android:onClick="homebnt"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/a"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="0dp"
        app:layout_constraintTop_toTopOf="@+id/toolbar"
        android:layout_marginTop="0dp"
        android:layout_marginLeft="24dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <ImageView
        android:id="@+id/Searchbnt"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/b"
        app:layout_constraintLeft_toRightOf="@+id/Homebnt"
        android:layout_marginLeft="24dp"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar"
        android:layout_marginBottom="0dp"
        app:layout_constraintTop_toTopOf="@+id/toolbar"
        android:layout_marginTop="0dp" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/iconppp"
        app:layout_constraintLeft_toRightOf="@+id/Searchbnt"
        android:layout_marginLeft="24dp"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar"
        android:layout_marginBottom="0dp"
        app:layout_constraintTop_toTopOf="@+id/toolbar"
        android:layout_marginTop="0dp" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/d"
        app:layout_constraintLeft_toRightOf="@+id/imageView9"
        android:layout_marginLeft="24dp"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar"
        android:layout_marginBottom="0dp"
        app:layout_constraintTop_toTopOf="@+id/toolbar"
        android:layout_marginTop="0dp" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/f"
        app:layout_constraintLeft_toRightOf="@+id/imageView10"
        android:layout_marginLeft="24dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="0dp"
        app:layout_constraintTop_toTopOf="@+id/toolbar"
        android:layout_marginTop="0dp" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/b"
        app:layout_constraintTop_toTopOf="@+id/toolbar2"
        android:layout_marginTop="0dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar2"
        android:layout_marginBottom="0dp" />

    <EditText
        android:id="@+id/editText5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/toolbar2"
        app:layout_constraintLeft_toRightOf="@+id/imageView12"
        android:layout_marginLeft="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar2"
        android:layout_marginBottom="8dp"
        app:layout_constraintTop_toTopOf="@+id/toolbar2"
        android:layout_marginTop="8dp" />
</android.support.constraint.ConstraintLayout>
