<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="sdu.thanaset.casephone.Home">

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@color/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <ImageView
        android:id="@+id/Homebnt"
        android:onClick="homebnt"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/a"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar3"
        app:layout_constraintTop_toTopOf="@+id/toolbar3"
        android:layout_marginLeft="24dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintVertical_bias="0.0"
        android:layout_marginStart="24dp" />

    <ImageView
        android:id="@+id/Searchbnt"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/b"
        android:onClick="searchbnt"
        app:layout_constraintLeft_toRightOf="@+id/Homebnt"
        android:layout_marginLeft="24dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="@+id/toolbar3"
        android:layout_marginStart="24dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/iconppp"
        app:layout_constraintLeft_toRightOf="@+id/Searchbnt"
        android:layout_marginLeft="24dp"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar3"
        app:layout_constraintTop_toTopOf="@+id/toolbar3"
        android:layout_marginStart="24dp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/d"
        app:layout_constraintLeft_toRightOf="@+id/imageView5"
        android:layout_marginLeft="24dp"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar3"
        app:layout_constraintTop_toTopOf="@+id/toolbar3"
        android:layout_marginStart="24dp" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/f"
        app:layout_constraintLeft_toRightOf="@+id/imageView6"
        android:layout_marginLeft="24dp"
        app:layout_constraintTop_toTopOf="@+id/toolbar3"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginStart="24dp" />

    <ScrollView
        android:layout_width="0dp"
        android:layout_height="495dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView5"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

        </LinearLayout>
    </ScrollView>

</android.support.constraint.ConstraintLayout>
